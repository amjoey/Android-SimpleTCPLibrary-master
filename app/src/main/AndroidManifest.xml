<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akexorcist.simpletcplibrary">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimpleTcp1Activity" />
        <activity android:name=".SimpleTcp2Activity" />
        <activity android:name=".SimpleTcp3MenuActivity" />
        <activity android:name=".SimpleTcp3ClientActivity" />
        <activity android:name=".SimpleTcp3ServerActivity" />
        <activity android:name=".ContinuousTcp1Activity" />
        <activity android:name=".ContinuousTcp2MenuActivity" />
        <activity android:name=".ContinuousTcp2ClientActivity" />
        <activity android:name=".ContinuousTcp2ServerActivity"></activity>
    </application>

</manifest>