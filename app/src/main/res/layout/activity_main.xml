<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.akexorcist.simpletcplibrary.MainActivity">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="vertical">

        <Button
            android:id="@+id/buttonSimpleTcp1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Simple TCP 1" />

        <Button
            android:id="@+id/buttonSimpleTcp2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Simple TCP 2" />

        <Button
            android:id="@+id/buttonSimpleTcp3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Simple TCP 3" />

        <Button
            android:id="@+id/buttonContinuous1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Continuous TCP 1" />

        <Button
            android:id="@+id/buttonContinuous2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Continuous TCP 2" />
    </LinearLayout>

</FrameLayout>
